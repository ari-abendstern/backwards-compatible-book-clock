{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "module": "node16",
    "moduleResolution": "node16",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "strict": true,
    "paths": {
      "@/*": ["./*"]
    },
  },
  "include": ["**/*.ts", "**/*.tsx", ".expo/types/**/*.ts", "expo-env.d.ts", "time-quotes.json"]
}
